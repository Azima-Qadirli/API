@model ICollection<API06.Service.DTOs.Category.CategoryGetDTO>
<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Name</th>
        <th scope="col">Delete</th>
        <th scope="col">Update</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <th scope="row">@item.Id</th>
            <td>@item.Name</td>
            <td>
                <a><i style="color:red" class="fa-solid fa-trash"></i></a>
            </td>
            <td>
                <a>
                    <i style="color:red" class="fa-solid fa-wrench"></i>
                </a>
            </td>
        </tr>
    }
    </tbody>
</table>